[Unit]
Description=GarageMaster

[Service]
ExecStart=/usr/local/bin/node /home/pi/scripts/garagemaster/bin/www
Restart=always
User=root
Group=root
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/pi/scripts/garagemaster

[Install]
WantedBy=multi-user.target